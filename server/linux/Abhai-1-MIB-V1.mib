--
-- Generic Trap Objects. These OIDs can be incorporated into the traps
-- listed below. 
--

       trapAmethystGuid OBJECT-TYPE
           SYNTAX    OCTET STRING (SIZE (1..255))
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
               "The deviceID of the device with the trap condition."
           ::= { amethystTraps 1 }

       trapHostGuid OBJECT-TYPE
           SYNTAX    OCTET STRING (SIZE (35..50))
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
               "The GUID of a host, could be src, intermediate or destination."
           ::= { amethystTraps 5 }

       trapVolumeGuid OBJECT-TYPE
           SYNTAX    OCTET STRING (SIZE (1..255))
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
               "The GUID of a Volume."
           ::= { amethystTraps 10 }

       trapAgentErrorMessage OBJECT-TYPE
           SYNTAX    OCTET STRING (SIZE (1..65535))
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
               "Error messages logged by vx/fx agents."
           ::= { amethystTraps 50}

	   trapAgentLoggedErrorLevel OBJECT-TYPE
           SYNTAX    OCTET STRING (SIZE (1..255))
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
             "Error level of logged messages by vx/fx agents. Ex: fatal/error"
         ::= { amethystTraps 55}

       trapAgentLoggedErrorMessageTime OBJECT-TYPE
           SYNTAX    OCTET STRING (SIZE (1..50))
           ACCESS    read-only
           STATUS    mandatory
           DESCRIPTION
              "Time at which error message was logged by vx/fx agent. Ex: YYYY-MM-DD HH:MM:SS"
           ::= { amethystTraps 60}

