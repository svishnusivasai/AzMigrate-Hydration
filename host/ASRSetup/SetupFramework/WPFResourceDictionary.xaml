    <!--Custom Button Style-->
    <Style
        x:Key="HyperLinkButton"
        x:Uid="Style_1"
        TargetType="{x:Type Button}">
        <Setter
            x:Uid="Setter_1"
            Property="Foreground"
            Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" />
        <Setter
            x:Uid="Setter_2"
            Property="Template">
            <Setter.Value>
                <ControlTemplate
                    x:Uid="ControlTemplate_1"
                    TargetType="{x:Type Button}">
                    <Grid
                        x:Uid="Grid_1"
                        Height="Auto"
                        Width="Auto">
                        <ContentPresenter
                            x:Uid="ContentPresenter_1"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            Margin="{TemplateBinding Padding}"
                            RecognizesAccessKey="True"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger
                x:Uid="Trigger_1"
                Property="IsMouseOver"
                Value="True">
                <Setter Property="Cursor" Value="Hand" />
                <Setter
                    x:Uid="Setter_3"
                    Property="BitmapEffect">
                    <Setter.Value>
                        <OuterGlowBitmapEffect
                            x:Uid="OuterGlowBitmapEffect_1"
                            GlowColor="LightBlue"
                            GlowSize="3" />
                    </Setter.Value>
                </Setter>
            </Trigger>
        </Style.Triggers>
    </Style>
