										x:Uid="Setter_10"
										Property="Visibility"
										TargetName="LeftDark"
										Value="Collapsed" />
									<Setter
										x:Uid="Setter_11"
										Property="Visibility"
										TargetName="RightDark"
										Value="Collapsed" />
									<Setter
										x:Uid="Setter_12"
										Property="Visibility"
										TargetName="LeftLight"
										Value="Collapsed" />
									<Setter
										x:Uid="Setter_13"
										Property="Visibility"
										TargetName="CenterLight"
										Value="Collapsed" />
									<Setter
										x:Uid="Setter_14"
										Property="Visibility"
										TargetName="RightLight"
										Value="Collapsed" />
									<Setter
										x:Uid="Setter_15"
										Property="Visibility"
										TargetName="Indicator"
										Value="Collapsed" />
								</Trigger>
								<Trigger
									x:Uid="Trigger_3"
									Property="IsIndeterminate"
									Value="false" />
							</ControlTemplate.Triggers>
						</ControlTemplate>
					</Setter.Value>
