										<Setter.Value>
											<RotateTransform
												x:Uid="RotateTransform_4"
												Angle="-90" />
										</Setter.Value>
									</Setter>
								</Trigger>
								<Trigger
									x:Uid="Trigger_2"
									Property="IsIndeterminate"
									Value="true">
